{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CaskrDatabaseConnectionString": ""
  },
  "SendGrid": {
    "ApiKey": "",
    "FromEmail": "support@caskr.co",
    "FromName": "caskr"
  },
  "Jwt": {
    "Key": "",
    "Issuer": "Caskr",
    "Audience": "CaskrUser"
  },
  "Keycloak": {
    "Authority": "http://localhost:8080",
    "Realm": "master",
    "ClientId": "caskr-client",
    "ClientSecret": "",
    "AdminUser": "",
    "AdminPassword": ""
  },
  "MobileRedirect": {
    "Enabled": false,
    "MobileDomain": "m.caskr.co",
    "DesktopDomain": "caskr.co",
    "RedirectStatusCode": 302,
    "MobileBypassParameter": "nomobile",
    "DesktopBypassParameter": "nodesktop",
    "PreferenceCookieName": "caskr_site_pref",
    "PreferenceCookieExpirationDays": 365,
    "ExcludedPaths": [
      "/api/",
      "/health",
      "/healthz",
      "/.well-known/",
      "/signin-oidc",
      "/signout-callback-oidc",
      "/auth/callback",
      "/oauth/callback"
    ],
    "ExcludedExtensions": [
      ".js",
      ".css",
      ".png",
      ".jpg",
      ".jpeg",
      ".gif",
      ".svg",
      ".ico",
      ".woff",
      ".woff2",
      ".ttf",
      ".eot",
      ".map",
      ".json",
      ".xml",
      ".txt",
      ".webp",
      ".mp4",
      ".webm",
      ".pdf"
    ]
  },
  "PushNotifications": {
    "VapidSubject": "mailto:admin@caskr.co",
    "VapidPublicKey": "",
    "VapidPrivateKey": ""
  }
}
